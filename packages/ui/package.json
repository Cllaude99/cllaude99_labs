{
  "name": "@cllaude99/ui",
  "version": "0.2.5",
  "description": "Cllaude99 Design System",
  "keywords": [
    "react",
    "ui",
    "design-system",
    "components",
    "emotion",
    "typescript"
  ],
  "author": "cllaude99",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cllaude99/cllaude99_labs.git",
    "directory": "packages/ui"
  },
  "sideEffects": false,
  "exports": {
    "./*": "./src/components/*/index.tsx",
    "./design-system/*": "./src/design-system/*.ts",
    "./hooks/*": "./src/hooks/*.ts"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "echo 'No build needed in development - using src directly'",
    "clean": "rm -rf dist",
    "lint": "eslint \"**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm clean && pnpm build",
    "storybook": "storybook dev -p 6006 -c storybook/.storybook",
    "build-storybook": "storybook build -c storybook/.storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "motion": "12.20.1",
    "react": "^18.2.0",
    "react-hot-toast": "2.6.0"
  },
  "devDependencies": {
    "@cllaude99/eslint-config": "workspace:*",
    "@cllaude99/typescript-config": "workspace:*",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.0",
    "storybook": "8.6.14",
    "the-new-css-reset": "^1.11.3",
    "typescript": "^5.5.4",
    "vite": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
